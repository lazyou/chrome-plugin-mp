{
  "manifest_version":2,
  "name":"Note",
  "description":"This is a description.",
  "version":"1.0",
  "browser_action":{
    "default_icon":"icon.png",
    "default_popup":"popup.html"
  },
  "content_scripts":[
    {
      "matches":[
        "<all_urls>"
      ],
      "js":[
        "js/lib/jquery-1.8.3.js",
        "js/content-script.js"
      ],
      "run_at":"document_start"
    }
  ],
  "web_accessible_resources": [
    "iframe-note-jquery/index.html"
  ],
  "background":{
    "scripts":[
      "js/background.js"
    ]
  },
  "permissions":[
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*"
  ]
}